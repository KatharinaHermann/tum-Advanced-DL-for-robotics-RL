{ 
    "env": {
        "name": "pointrobo-v0",
        "normalize": true,
        "action_low": -1.0,
        "action_high": 1.0,
        "goal_reward": 5,
        "collision_reward": -1,
        "step_reward": -0.01,
        "buffer_size": 100,
        "grid_size": 32,
        "num_obj_max": 5, 
        "obj_size_avg": 8, 
        "robot_radius": 1,
        "max_goal_dist": null,
        "WS_level": "middle" 
    },
    "cae": {
        "pooling": "max",
        "latent_dim": 16,
        "conv_filters": [4, 8, 16],
        "weights_path": "models/cae/model_num_5_size_8.h5"
    },
    "agent": {
        "name": "DDPG",
        "max_grad": 0.6,
        "lr_actor": 0.0003,
        "lr_critic": 5e-5,
        "actor_units": [400, 300],
        "critic_units": [400, 300],
        "sigma": 0.1,
        "tau": 0.004,
        "n_warmup": 1e4,
        "memory_capacity": 1e6,
        "batch_size": 256,
        "update_interval": 1,
        "gpu": 0,
        "lr_decay_steps": [3e5],
        "actor_lr_decay_vals": [3e-5],
        "critic_lr_decay_vals": [1e-5]
    },
    "trainer": {
        "train_from_scratch" : true,
        "mode": "train",
        "relabeling_mode": "random",
        "remove_zigzaging": false,
        "max_steps": 1e6,
        "episode_max_steps": 50,
        "n_experiments": 1,
        "show_progress": false,
        "show_progress_interval": 100,
        "save_model_interval": 1e4,
        "save_summary_interval": 1e3,
        "logdir": "results",
        "logging_level": "INFO",
        "model_dir": "models/agents",
        "dir_suffix": "",
        "use_prioritized_rb": false,
        "use_nstep_rb": false,
        "n_step": 4,
        "test_interval": 1e4,
        "show_test_progress": true,
        "test_episodes": 100,
        "save_test_path": false,
        "save_test_path_sep": false,
        "save_test_movie": false,
        "show_test_images": false,
        "normalize_obs": false,
        "show_relabeling": true
    }
}