{
    "env": {
        "name": "pointrobo-v0",
        "goal_reward": 5,
        "collision_reward": -1,
        "step_reward": -0.01,
        "buffer_size": 100,
        "grid_size": 32,
        "num_obj_max": 5,
        "obj_size_avg": 8, 
        "robot_radius": 1,
        "max_goal_dist": null
    },
    "trainer": {
