{
    "env": {
        "name": "pointrobo-v0",
        "goal_reward": 15,
        "collision_reward": -1,
        "step_reward": -0.1,
        "buffer_size": 100,
        "grid_size": 32,
        "num_obj_max": 0,
        "obj_size_avg": 8, 
        "robot_radius": 1,
        "max_goal_dist": 4
    },
    "cae": {
        "pooling": "max",
        "latent_dim": 16,
        "conv_filters": [4, 8, 16],
        "weights_path": "models/cae//model_num_5_size_8.h5"
    },
    "agent": {
        "name": "DDPG",
        "max_grad": 0.5,
        "lr_actor": 5e-6,
        "lr_critic": 5e-6,
        "actor_units": [400, 300],
        "critic_units": [400, 300],
        "sigma": 0.1,
        "tau": 0.005,
        "n_warmup": 1e4,
        "memory_capacity": 1e6,
        "batch_size": 256,
        "update_interval": 1,
        "gpu": 0
    },
    "trainer": {
        "train_from_scratch" : true,
        "evaluate": false,
        "max_steps": 1e6,
        "episode_max_steps": 10,
        "n_experiments": 1,
        "show_progress": true,
        "save_model_interval": 1e4,
        "save_summary_interval": 1e3,
        "logdir": "results",
        "logging_level": "INFO",
        "model_dir": "models/agents",
        "dir_suffix": "",
        "use_prioritized_rb": false,
        "use_nstep_rb": false,
        "n_step": 4,
        "test_interval": 1e4,
        "show_test_progress": false,
        "test_episodes": 100,
        "save_test_path": false,
        "save_test_path_sep": false,
        "save_test_movie": false,
        "show_test_images": false,
        "normalize_obs": false
    }
}